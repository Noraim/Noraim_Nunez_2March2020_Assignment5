import math
import statistics

num_array = list()
num = input("Enter your desired number of elements:")
print("Enter your desired numbers in the array")
for i in range(int(num)):
   n = input("num:")
   num_array.append(int(n))
print("Array:",num_array)

print("Standard Deviation of Array:  % s "
            %(statistics.stdev(num_array)))

